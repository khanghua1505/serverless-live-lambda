{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "lib": [
      "es2019",
      "es2020.promise",
      "es2020.bigint",
      "es2021.string"
    ],
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "rootDir": "src/",
    "outDir": "dist",
  },
  "include": [
    "src/**/*.ts",
  ]
}